{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "fideloper/proxy": "4.3.*",
        "fruitcake/laravel-cors": "1.0.*",
        "guzzlehttp/guzzle": "6.5.*",
        "laracasts/flash": "3.1.*",
        "laravel/framework": "7.1.*",
        "laravel/ui": "2.0.*",
        "tightenco/ziggy": "^0.9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "2.0.*",
        "fzaninotto/faker": "1.9.*",
        "laravel/homestead": "10.5.*",
        "laravel/tinker": "2.2.*",
        "mockery/mockery": "1.3.*",
        "nunomaduro/collision": "4.1.*",
        "phpunit/phpunit": "9.0.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi",
            "php artisan ziggy:generate resources/js/ziggy.js"
        ]
    }
}
